# Environment variables for Docker deployment
# Copy this file to .env and update with your values

# Domain configuration
DOMAIN=docs.waffle.food
SSL_EMAIL=admin@waffle.food

# Traefik configuration (if using SSL)
TRAEFIK_DASHBOARD_DOMAIN=traefik.waffle.food

# Node.js environment
NODE_ENV=production

# Optional: Custom port (default: 3000)
DOCS_PORT=3000

# Optional: Enable/disable services
ENABLE_TRAEFIK=true
ENABLE_SSL=true
